{
    "tuid": {
        "database": {
            "name": "TUID.db"
        },
        "local_hg_source": "C:/mozilla-source/mozilla-central/",
        "hg_for_building": "C:/mozilla-build/python/Scripts/hg.exe",
        "hg": {
            "branch": "mozilla-central"
        },
        "hg_cache": {},
        "_hg_cache": {
            "use_cache": true,
            "hg": {
                "url": "https://hg.mozilla.org"
            },
            "branches": {
                "host": "http://localhost",
                "port": 9200,
                "index": "branches",
                "type": "branch",
                "tjson": false,
                "timeout": 300,
                "consistency": "one", // WE ONLY NEED ONE SHARD TO BE AVAILABLE
                "debug": false,
                "limit_replicas": false
            },
            "repo": {
                "host": "http://localhost",
                "port": 9200,
                "index": "repo",
                "type": "revision",
                "tjson": false,
                "timeout": 300,
                "consistency": "one", // WE ONLY NEED ONE SHARD TO BE AVAILABLE
                "debug": false,
                "limit_replicas": false
            }
        }
    },
    "constants": {
        "pyLibrary.env.http.default_headers": {"Referer": "https://github.com/mozilla/TUID"}
    },
    "debug": {
        "trace": true
    }
}
